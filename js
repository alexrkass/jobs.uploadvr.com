<script>
//LOAD FAVICON
(function() {
    var link = document.createElement('link');
    link.type = 'image/x-icon';
    link.rel = 'shortcut icon';
    link.href = 'http://uploadvr.com/wp-content/uploads/2015/10/FAVICON-1.ico';
    document.getElementsByTagName('head')[0].appendChild(link);
}());

//LOAD FONTS
WebFontConfig = {
  google: { families: [ 'Roboto+Slab:400,300:latin', 'Roboto:400,300' ] }
};
(function() {
  var wf = document.createElement('script');
  wf.src = ('https:' == document.location.protocol ? 'https' : 'http') +
    '://ajax.googleapis.com/ajax/libs/webfont/1/webfont.js';
  wf.type = 'text/javascript';
  wf.async = 'true';
  var s = document.getElementsByTagName('script')[0];
  s.parentNode.insertBefore(wf, s);
})();

function hideSignupPopup(e,poppedup) {
  $('#signup-popup').remove();
  $('#popup-close-button').remove();
  poppedup = false;
}
function displaySignupPopup(e,poppedup) {
  e.preventDefault();
  $('body').append("<div id=signup-popup><div id=popup-content-container><div id=popup-header-container><p id=popup-header>JOIN OUR COMMUNITY</p></div><br><a href='http://jobs.uploadvr.com/profiles/sign_up'><div class= popup-link id=popup-link-1><div id = popup-link-1-content><p class=popup-link-text>I'm a Job Seeker</p><img src=http://uploadvr.com/wp-content/uploads/2015/11/Arrow@2x.png class=popup-link-arrow></div></div></a><br><a href='http://jobs.uploadvr.com/employers/sign_up'><div class= popup-link id=popup-link-2><div id=popup-link-2-content><p class=popup-link-text>I'm an Employer</p><img src=http://uploadvr.com/wp-content/uploads/2015/11/Arrow@2x.png class=popup-link-arrow></div></div></a></div><img id=popup-close-button src=http://uploadvr.com/wp-content/uploads/2015/11/x@2x.png></div>");
  $('#popup-close-button').on("click", function(e){hideSignupPopup(e,poppedup);});
  poppedup = true;

  $(document).mouseup(function (e)
{
    var container = $("#signup-popup");

    if (!container.is(e.target) // if the target of the click isn't the container...
        && container.has(e.target).length === 0) // ... nor a descendant of the container
    {
        container.remove();
         $('#popup-close-button').remove();
    }
});
}

function displaySigninPopup(e,poppedup) {
  e.preventDefault();
  $('body').append("<div id=signup-popup><div id=popup-content-container><div id=popup-header-container><p id=popup-header>SIGN IN</p></div><br><a href='http://jobs.uploadvr.com/profiles/sign_in'><div class= popup-link id=popup-link-1><div id = popup-link-1-content><p class=popup-link-text>I'm a Job Seeker</p><img src=http://uploadvr.com/wp-content/uploads/2015/11/Arrow@2x.png class=popup-link-arrow></div></div></a><br><a href='http://jobs.uploadvr.com/employers/sign_in'><div class= popup-link id=popup-link-2><div id=popup-link-2-content><p class=popup-link-text>I'm an Employer</p><img src=http://uploadvr.com/wp-content/uploads/2015/11/Arrow@2x.png class=popup-link-arrow></div></div></a></div><img id=popup-close-button src=http://uploadvr.com/wp-content/uploads/2015/11/x@2x.png></div>");
  $('#popup-close-button').on("click", function(e){hideSignupPopup(e,poppedup);});
  poppedup = true;

  $(document).mouseup(function (e)
{
    var container = $("#signup-popup");

    if (!container.is(e.target) // if the target of the click isn't the container...
        && container.has(e.target).length === 0) // ... nor a descendant of the container
    {
        container.remove();
         $('#popup-close-button').remove();
    }
});
}
