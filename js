  //SETUP AJAX CALL FOR FILTER
    var filterAjaxCall = function(e){
      e.preventDefault();
      var category = $("#c_chosen a span").text();
      var category_value = "";
      var categories = $("#c option");
      category_value = getURLParamFromSelectOption(category,category_value,categories);
      var location = $("#l_chosen a span").text();
      var location_value = "";
      var locations = $("#l option");
      location_value = getURLParamFromSelectOption(location,location_value,locations);
      var jobtype = $("#jt_chosen a span").text();
      var jobtype_value = "";
      var jobtypes = $("#jt option");
      jobtype_value = getURLParamFromSelectOption(jobtype,jobtype_value,jobtypes);

      $.ajax({
        dataType : 'html',
        url: '/jobs?utf8=âœ“&q=&l='+location_value+'&c='+category_value+'&jt='+jobtype_value,
      }).success(function(data){
          var jobs = $(data).find("#jobs-table");
          $("#jobs-table").html(jobs);
          $(".date>div").each(function(index){
            var str = $(".location:eq("+index+")").text();
            var patt = /[A-Z a-z]+,[A-Z a-z]+/;
            var res = patt.exec(str);
            if (res !== null){
              $(this).text(res);
            } else{
              $(this).text(str);
            }
      });
      $(".location").remove();
      $('.job>.row').prepend("<div class='col-sm-1'></div>");
      $(".row>.details").removeClass("col-sm-10");
      $(".row>.details").addClass("col-sm-7");
      $(".details>.details").removeClass("col-sm-9");
      $('.date').addClass('col-sm-3').removeClass('col-sm-2');
      $('.date').css('top','14px');
      if ($(".btn, .btn-default, .dropdown-toggle").first().text().indexOf("Job Seekers") >= 0) {
       $(".job").on("click",function(e){displaySignupPopup(e,poppedup)});
      }
      $(".job").mouseover(function(){$(".chosen-container").removeClass("chosen-with-drop")});
      });
    }
    $("select#q,select#c,select#l,select#jt").on("change",function(e){filterAjaxCall(e)});

    if($(window).width() > 500){
    $(".btn-purchase-custom").css("border-radius","0px");
    $(".well").eq(2).remove();
