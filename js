function getURLParamFromSelectOption(text, option_value, selector){
  $.each(selector, function(){
                  if ($(this).text() === text){
                    return option_value = $(this).attr("value");
                  }
                })
  console.log("option value:"+option_value);
  return option_value;
}
function datesToLocations(){
  $(".date>div").each(function(index){
    var str = $(".location:eq("+index+")").text();
    var patt = /[A-Z a-z]+,[A-Z a-z]+/;
    var res = patt.exec(str);
    if (res !== null){
      $(this).text(res);
    } else{
      $(this).text(str);
    }
  })
  $(".location").remove();
};

$(document).ready(function(){
  $('body').append('<div id="fb-root"></div>');

  //IF PROFILE PAGE, REMOVE 'you are already signed in'
  if(document.querySelector('#profile-form') !== null){
  $(".system-alert").remove();
  }
  datesToLocations();

  //SNIFF DEFAULT SEARCH TO DYNAMICALLY UPDATE CATEGORIES FILTER
  var c = $("select#c").prop('outerHTML');

  //TEST IF LOGGED IN, APPEND EXTRA NAV LINKS
   if ($(".btn, .btn-default, .dropdown-toggle").first().text().indexOf("Job Seekers") >= 0) {
    $(".navbar-nav").children().first().children().attr("id","sign-in");

    $(".navbar-nav").append("<li class><a href='http://jobs.uploadvr.com/profiles/sign_up' id=sign-up>Sign Up</a></li>");
    var poppedup = false;
    $(".job, #sign-up").on("click",function(e){displaySignupPopup(e,poppedup)});
    $("#sign-in").on("click",function(e){displaySigninPopup(e,poppedup)});
      }
  else{
    $(".navbar-nav").html("<li class><a href='http://jobs.uploadvr.com/profiles/sign_up'> My Profile</a></li>");
    }

  //MODIFY PROFILE PAGE ORGANIZATION
  if (document.querySelector(".profile-core-info") !== null && $(window).width() > 500){
    $('.profile-message').detach().appendTo('.profile-core-info');
    $(".profile-body .row").detach().appendTo('.profile-header');
    $('.profile-body').remove();
  }

  //REPLACE DEFAULT SEARCH WITH CHOSEN SEARCH AND MODIFY JOBS INDEX PAGE STYLING
